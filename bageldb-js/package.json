{
  "name": "@bageldb/bagel-db",
  "version": "0.3.19-alpha.0.3",
  "description": "A simple way to communicate with BagelDB, wrapping the REST http endpoints in easy to use functions",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "exports": {
    "require": "./dist/index.cjs",
    "default": "./dist/index.cjs",
    "import": "./dist/index.js"
  },
  "types": "types",
  "scripts": {
    "expose:cmd": "node unwrap.js",
    "build": "webpack --mode=production",
    "prepare": "npm run build",
    "start:dev": "webpack --mode=development --watch --progress",
    "build:prod": "webpack --mode=production",
    "prepublishOnly:old": "webpack --mode=production && npm run expose:cmd",
    "prepublishOnly:old:dev": "npm run clean & webpack --mode=production && npm run expose:cmd",
    "prepublishOnly": "webpack --mode=production",
    "prepublishOnly:dev": "npm run clean & webpack --mode=production",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\"",
    "lint:fix": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "clean": "rm index.js.LICENSE.txt bageldb.js.LICENSE.txt es-bagel.js shared.js spread.d.ts spread.js index.html bageldb.js index.js common.d.ts interfaces.d.ts users.d.ts index.d.ts bagelDBRequest.d.ts bagelMetaRequest.d.ts fbStorage.d.ts",
    "publish:prod": "npm publish --access public --tag alpha",
    "publish:dev": "npm run clean & npm publish --access public --dry-run --tag alpha"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bageldb/bagel-db-libraries.git"
  },
  "keywords": [
    "CMS",
    "BagelDB"
  ],
  "author": "BagelStudio",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/bageldb/bagel-db-libraries/issues"
  },
  "homepage": "https://docs.bageldb.com",
  "dependencies": {
    "@babel/polyfill": "^7.10.1",
    "axios": "^0.25.0",
    "form-data": "^4.0.0",
    "lodash-es": "^4.17.21"
  },
  "devDependencies": {
    "@types/eslint": "^8.2.2",
    "@types/lodash-es": "^4.17.6",
    "@types/node": "^17.0.9",
    "@types/webpack-node-externals": "^2.5.3",
    "@typescript-eslint/eslint-plugin": "^5.10.0",
    "@typescript-eslint/parser": "^5.10.0",
    "clean-webpack-plugin": "^4.0.0",
    "esbuild": "^0.14.27",
    "eslint": "^8.7.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "expose-loader": "^3.1.0",
    "html-webpack-plugin": "^5.5.0",
    "prettier": "^2.5.1",
    "semver": "^7.3.5",
    "ts-loader": "^9.2.6",
    "tslib": "^2.3.1",
    "typescript": "^4.5.4",
    "webpack": "^5.66.0",
    "webpack-cli": "^4.9.1",
    "webpack-node-externals": "^3.0.0"
  }
}